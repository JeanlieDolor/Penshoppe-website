  const event_change = new Event('change');

  function minimum_length(elem, len) {
    if (elem.id = 'house-code') {
      if (elem.value.length < len) {
        elem.style.border = '1px solid #FF0000';
        return false;
      }
    } else {
      if (elem.value.length != len) {
        elem.style.border = '1px solid #FF0000';
        return false;
      }
    }
  }

  var storeAddress = getStoredAddress();

  function cancelBtn() {
